{
  "name": "kline-next",
  "version": "0.0.1",
  "private": true,
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --ignore-path ./.gitignore --write "
    ],
    "*.{js,ts,cjs,mjs,jsx,tsx,json}": [
      "biome lint --write --no-errors-on-unmatched"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "npm run copy-static && cross-env PORT=$npm_config_port HOSTNAME=$npm_config_host node .next/standalone/server.js",
    "build:static": "npm run build && npm run copy-static && npm run copy-serve",
    "copy-static": "cpx .next/static/**/* .next/standalone/.next/static && cpx public/**/* .next/standalone/public",
    "copy-serve": "cpx .next/standalone/**/* ./.next/.output/ && cpx .next/static/**/* ./.next/.output/.nexts/static/",
    "lint": "biome lint . --no-errors-on-unmatched",
    "lint:fix": "pnpm run lint --write"
  },
